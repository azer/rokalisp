#!/usr/bin/node

var lisp = require('../src/lisp'),
    readFile = require('fs').readFile,
    puts = require('sys').puts;

global.puts = puts;

process.argv.slice(2).forEach(function(fl){
  
  readFile(fl,function(err,ctx){
    if(err) throw err;
    console.log('read>>',lisp.read(lisp.tokenize(ctx)));
  });

});
